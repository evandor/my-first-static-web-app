  <div>
    <div class="section-header sub-headline1">IOL Calculations</div>

    <div class="flexbox-2">
      <div>
        <div
          class="h3 iol-line ellipsis toric" lines="2" min-font-size="7">{{ model.recommended.iol.name | default: "*ZEISS AT LISA tri toric 949MP" }}
        </div>
        {% assign favoredIol = model.recommended.iols | where: "favored", true | first %}
        {% echo favoredIol%}
        <div class="sub-headline2 margin-3">
          <span id="">{{ favoredIol.nonToric.se | default: "*+20.50 D"}}</span>
          {% if model.toricLens %}
            <span id=""> ({{ favoredIol.toric.cylinder | default: "*2.50"}} @ {{ favoredIol.toric.axis | default: "*86°"}})</span>
          {% endif %}
        </div>
      </div>
      <div>
        <div>
          <div class="simple-line">
            <div class="left">
              <span id="">Target Range</span>
            </div>
            <div class="right">
              <span id="">{{- model.recommended.iol.targetRange | default: "*Intermediate" }}</span>
            </div>
          </div>
          <div class="simple-line body1">
            <div class="left">
              <span id="">Target SE</span>
            </div>
            <div class="right">
              <span id="">{{ model.recommended.iol.targetSe | default: "*-1.75 D" }}</span>
            </div>
          </div>
          <div class="simple-line body1">
            <div class="left">
              <span id="">Predicted Refraction</span>
            </div>
            <div class="right">
              <span id="">{{ model.recommended.iol.predictedRefraction | default: "*-0.24+ 0.41 x 5° (SE -0.03)" }}</span>
            </div>
          </div>
          
          <div class="simple-line body1">
            <div class="left">
              <span id="">Formula used</span>
            </div>
            <div class="right">
              <span id="">{{ model.recommended.iol.formulaUsed | default: "*Z CALC | Total Keratometry" }}</span>
            </div>
          </div>
        </div>
        <div class="simple-line sub-headline2 margin-3" style="display: none;">
          <div class="left">
            <span id="">Z CALC | Total Keratometry</span>
          </div>
          <div class="right">
            <span id=""></span>
          </div>
        </div>
        <div class="flexbox-2 iol-calc">
          <div>
            <table class="spheric-table normal-text ">
              <tbody>
                <tr>
                  <th scope="row">
                    <span>
                      <span>IOL</span>
                    </span>
                    <!-- NonBreakingSpace necessary to ensure vertical alignment of result boxes -->
                    <span class="label2-small">SE</span>
                  </th>
                  <th scope="row">Res. SE</th>
                </tr>
                {%- for candidateIol in model.recommended.iols -%}
                  <tr class="{% if candidateIol.favored %}body1{% else %}normal-text{% endif %}">
                    <td>{{ candidateIol.nonToric.se }}</td>
                    <td>{{ candidateIol.nonToric.resRefSe }}</td>
                  </tr>
                {% endfor %}
                {%- comment -%} Bth: replaced static layout with dynamic layout and reading of model data
                <tr class="normal-text">
                  <td>+21.00</td>
                  <td>-0.20</td>
                </tr>
                <tr class="body1">
                  <td>+20.50</td>
                  <td>+0.17</td>
                </tr>
                <tr class="normal-text">
                  <td>+20.00</td>
                  <td>+0.53</td>
                </tr>
                {%- endcomment -%}
                {%- comment -%} <tr class="normal-text-small">
                <td>+20.73</td>
                <td>0.00</td>
                </tr> {%- endcomment -%}
              </tbody>
            </table>
          </div>
          <div>
            {%  if model.toricLens %}
            <table class="toric-table normal-text">
              <tbody>
                <tr>
                  <th>Toric Power</th>
                  <th colspan="4">Res. Ast.</th>
                </tr>
                {% for candidateIol in model.recommended.iols %}
                  <tr class="{% if candidateIol.favored %}body1{% else %}normal-text{% endif %}">
                    <td class="toric-power">{{ candidateIol.toric.cylinder | default : "*+2.25"}}</td>
                    <td class="res-ast">{{ candidateIol.toric.resRefCylinder | default : "*+0.10"}}</td>
                    <td>@</td>
                    <td>{{ candidateIol.toric.resRefAxis | default : "*111°"}}</td>
                    <td class="axis-flip"></td>
                  </tr>
                {% endfor %}
                {%- comment -%}
                <tr class="normal-text">
                  <td class="toric-power">+2.25</td>
                  <td class="res-ast">+0.10</td>
                  <td>@</td>
                  <td>111°</td>
                  <td class="axis-flip"></td>
                </tr>
                <tr class="body1">
                  <td class="toric-power">+2.25</td>
                  <td class="res-ast">+0.27</td>
                  <td>@</td>
                  <td>21°</td>
                  <td class="axis-flip"></td>
                </tr>
                <tr class="normal-text">
                  <td class="toric-power">+2.25</td>
                  <td class="res-ast">+0.63</td>
                  <td>@</td>
                  <td>21°</td>
                  <td class="axis-flip"></td>
                </tr>
                {%- endcomment -%}
              </tbody>
            </table>
            {%  endif  %}
          </div>
        </div>
      </div>
    </div>
