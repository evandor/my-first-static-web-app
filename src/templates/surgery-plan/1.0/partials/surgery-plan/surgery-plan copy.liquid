                    {%- else -%}
                      <img src="{{ versionedReportBaseUrl }}/assets/images/check-box-empty.png" alt="Checkbox" />
                    {%- endif -%}
                  </div>
                </td>
              </tr>
            {% endfor %}
            {%- comment -%}
            <tr>
              <td>+25.50</td>
              <td>-1.79</td>
              <td>
                <div class="check-box">
                  <img src="{{ versionedReportBaseUrl }}/assets/images/check-box-empty.png" alt="Checkbox" />
                </div>
              </td>
            </tr>
            <tr>
              <td>+23.50</td>
              <td>-1.87</td>
              <td>
                <div class="check-box">
                  <img src="{{ versionedReportBaseUrl }}/assets/images/check-box-empty.png" alt="Checkbox" />
                </div>
              </td>
            </tr>
            <tr>
              <td>+21.50</td>
              <td>-1.85</td>
              <td>
                <div class="check-box">
                  <img src="{{ versionedReportBaseUrl }}/assets/images/check-box-empty.png" alt="Checkbox" />
                </div>
              </td>
            </tr>
            {%- endcomment -%}
          </tbody>
        </table>
      </div>
    </div>
  </div>

